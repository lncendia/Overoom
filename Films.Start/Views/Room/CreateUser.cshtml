@model Overoom.WEB.Contracts.Rooms.CreateYoutubeRoomParameters

<form asp-action="CreateUser" method="post" class="form-horizontal">
    <div asp-validation-summary="ModelOnly"></div>
    <div class="form-group">
        <div class="row g-3">
            <div class="col-12">
                <label asp-for="Url"></label>
                <input asp-for="Url" class="form-control"/>
                <span asp-validation-for="Url" class="text-danger"></span>
            </div>
            <div class="col-12">
                <label asp-for="IsOpen"></label>
                <select asp-for="IsOpen" class="form-control">
                    <option selected="selected" value="">Не выбрано</option>
                    <option value="@true">Открытая</option>
                    <option value="@false">Закрытая</option>
                </select>
                <span asp-validation-for="IsOpen" class="text-danger"></span>
            </div>
            <div class="col-12">
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="remember" asp-for="Access">
                    <label asp-for="Access" class="form-check-label" for="remember"></label>
                </div>
            </div>
            <div class="col-12">
                <input class="btn shadow-none btn-dark" type="submit" value="Сохранить"/>
            </div>
        </div>
    </div>
</form>